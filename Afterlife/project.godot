[application]

config/name="Beyond: Soul Ascension"
run/main_scene="res://scenes/main_scene.tscn"
config/icon="res://icon.png"

[autoload]

Anima="*res://addons/anima/core/anima.gd"
GLoot="*res://addons/gloot/gloot_autoload.gd"
